#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rakeshg/test/101-school/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.3.101_@swc+helpers@0.5.5__@types+node@20.12.7_typescript@5.4.5/node_modules/ts-node/dist/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.3.101_@swc+helpers@0.5.5__@types+node@20.12.7_typescript@5.4.5/node_modules/ts-node/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.3.101_@swc+helpers@0.5.5__@types+node@20.12.7_typescript@5.4.5/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rakeshg/test/101-school/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.3.101_@swc+helpers@0.5.5__@types+node@20.12.7_typescript@5.4.5/node_modules/ts-node/dist/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.3.101_@swc+helpers@0.5.5__@types+node@20.12.7_typescript@5.4.5/node_modules/ts-node/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.3.101_@swc+helpers@0.5.5__@types+node@20.12.7_typescript@5.4.5/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-node@10.9.2_@swc+core@1.3.101_@swc+helpers@0.5.5__@types+node@20.12.7_typescript@5.4.5/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-node@10.9.2_@swc+core@1.3.101_@swc+helpers@0.5.5__@types+node@20.12.7_typescript@5.4.5/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
fi
