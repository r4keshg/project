#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rakeshg/test/101-school/node_modules/.pnpm/vitest@1.6.1_@types+node@20.12.7_terser@5.39.0/node_modules/vitest/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/vitest@1.6.1_@types+node@20.12.7_terser@5.39.0/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rakeshg/test/101-school/node_modules/.pnpm/vitest@1.6.1_@types+node@20.12.7_terser@5.39.0/node_modules/vitest/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/vitest@1.6.1_@types+node@20.12.7_terser@5.39.0/node_modules:/home/rakeshg/test/101-school/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitest@1.6.1_@types+node@20.12.7_terser@5.39.0/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/vitest@1.6.1_@types+node@20.12.7_terser@5.39.0/node_modules/vitest/vitest.mjs" "$@"
fi
